library(tidyverse)
data(starwars)
# What is the mean height of humans by planet of birth?
function_ex1 <- function(data) {
data %>%
select(height, homeworld, species) %>%
filter(species == "Human") %>%
na.omit() %>%
group_by(homeworld) %>%
summarise(Mean_height = mean(height))
}
function_ex1(starwars)
